/* TOOLTIP-VALIDATION */

.tooltip-validation {
    background : #aa0606;
    color : #fff;
    position : absolute;
    top : -1px;
    right : 0;
    font-size : 10px;
    padding : 3px 5px;
    border-radius : 3px;
    font-weight : bold;
}


.tooltip-validation:after {
    content : " ";
    position : absolute;
    bottom : -5px;
    right : 15px;
    width : 0px;
    height : 0px;
    border-style : solid;
    border-width : 5px 4px 0 4px;
    border-color : #aa0606 transparent transparent transparent;
    z-index : 2;
}

.tooltip-validation.select-tooltip{
    right : -5px !important;
}

.tooltip-validation.div-tooltip{
    right : 10% !important;
}

input.ng-invalid, textarea.ng-invalid, select.ng-invalid, div.ng-invalid {
    border-color : #e9322d;
}

.dialog-delete .modal-header{
    background-color: #DA4F49;
    background-image: linear-gradient(to bottom, #EE5F5B, #BD362F);
    background-repeat: repeat-x;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    color: #FFFFFF;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
}

.gridFooterDiv {
    background-color: #EDEDED;
    border: none;
    text-align: center;
    height: 76px;
}

.cell-centered {
    padding-left: 5px;
    text-align: center;
    padding-top: 2px;
}

/* CSS geral - Modulo */
hr.line-separetor { border: 1px solid #d9d9d9; width: 100%}
#page-wrapper { padding: 10px 0; }
#page-wrapper .header-module { border-bottom: 1px solid #d9d9d9; height: 80px; padding-top: 15px; }
#page-wrapper .header-module .header-space { padding: 10px 40px; min-height: 50px;}
#page-wrapper .header-module .title-module { font-weight: bold; float: left; position: relative; top: 9px;  }
#page-wrapper .header-module .buttons-module { position: relative; left: 15px; top: 4px }
#page-wrapper .content-module .form-actions {margin: 15px 0;}
#page-wrapper .content-module { padding: 25px 40px; }
#page-wrapper .content-module .form-group ul { padding: 0 }
#page-wrapper .content-module .btn-search { width: 50px }
#page-wrapper .content-module .btn-equipamento { background-color: #FFAD00; }
#page-wrapper .content-module .tab-content {
    padding-top: 36px;
}

#page-wrapper .header-module .button {
    width: 1.333333%;
}

#page-wrapper .header-module .col-md-2 {
    width: 12.666667%;
}

.tab-disabled .nav-tabs > li > a:hover,
.nav-tabs > li > a:focus {
    border-color: #FFF;
    background-color: #FFF;
    cursor: default;
    color: gray;
}

#page-wrapper .content-module input[type=radio], input[type=checkbox] {
    width: 16px;
    height: 16px;
}

.language-option {
    margin-top: 10px;
}

.language-option .label-br {
    color: #00913F;
}

.language-option .label-py {
    color: #CD1126;
}

#page-wrapper label {
    display: -webkit-inline-box;
}

#page-wrapper .content-module .col-md-2 {
    width: 20.666667%;
}

.icon-br {
    background: url("../images/icon-br.png") no-repeat;
    width: 25px;
    height: 17px;
    margin-left: 11px;
    margin-right: 11px;
    margin-top: 2px;
    display: inline-block;
}

.icon-py {
    background: url("../images/icon-py.png") no-repeat;
    width: 25px;
    height: 17px;
    margin-left: 11px;
    margin-right: 11px;
    margin-top: 2px;
    display: inline-block;
}

.nav-tabs>li.active>a, .nav-tabs>li.active>a:hover, .nav-tabs>li.active>a:focus {
    border-top: none;
    border-left: none;
    border-right: none;
    border-bottom: 3px solid #0077BF;
    font-size: 13px;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: bold;
    color: #000;
}

.nav-tabs .active .badge {
    background-color: #0077BF;
}

.nav-tabs .badge {
    border-radius: 23px;
    height: 23px;
    padding: 5px 7px;
}

.nav-tabs {
    font-size: 13px;
    font-family: Arial, Helvetica, sans-serif;
}


@media (max-width: 1023px) {
    #page-wrapper .header-module .buttons-module { position: relative; left: 15px; }
    #page-wrapper .row div {display: block; clear: both; width: 100%; margin: 5px 0}
    #page-wrapper .row  select,#page-wrapper .row  input {width: inherit!important}
}

